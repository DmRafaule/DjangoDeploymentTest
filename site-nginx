server {
    listen      80;
    server_name beget-django-deployment-test.online www.beget-django-deployment-test.online;
    location /static {
        alias /home/dima/sites/www.staging.beget-django-deployment-test.online/static;
    }
    location /media {
        alias /home/dima/sites/www.staging.beget-django-deployment-test.online/media;
    }
    location / {
        proxy_set_header Host $host;
        proxy_pass http://unix:/tmp/beget-django-deployment-test.online.socket;
    }
}