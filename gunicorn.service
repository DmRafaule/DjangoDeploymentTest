[Unit]
    Description=Gunicorn server for beget-django-deployment-test.online
[Service]
    Restart=on-failure  
    User=dima  
    WorkingDirectory=/home/dima/sites/www.beget-django-deployment-test.online/source/Website  
    ExecStart= /home/dima/sites/www.beget-django-deployment-test.online/venv/bin/gunicorn --bind unix:/tmp/beget-django-deployment-test.online.socket Website.wsgi:application  
[Install]
    WantedBy=multi-user.target